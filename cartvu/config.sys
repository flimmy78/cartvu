AlarmID=    3                           ;//单告警音时的声音ID，0-5共六种声音可以选择
Alar mVolume=60                      ;//告警音量：0---99（max）
Alar mMsg=0                          ;//告警信息（图片闪动时，选择与告警图片交替显示的图片）0--CARTVU MEDIA图标，1--用户自定义
[ST ANDBY]
Stan dbyMode=0                       ;//待机模式，0--CARTVU模式，1--连续图像模式，2--滚动图片模式，3--自定义模式
RevolveRange=00--10                 ;//滚动图片时，滚动图片的ID区间，xx-yy，ID号从00--99
Rev olvePeriod=2                     ;//滚动图片时，图片滚动周期，单位：秒
[OT HER]  
ImageQu alit  y=1                      ;//转换成jpeg时的图片质量，0--高质量，1--中质量，2--低质量
#Extr aItemInfo= 0                     ;//是否填写附加商品信息，0--不填写，1--填写
#Reset Time     =       0 9 :1 0             ;//系统每天自动复位时间，hh:mm
