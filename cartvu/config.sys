    ;//配置文件的最后修改时间
[NETWORK]  ;
LocalServer =			192.168.1.3             ;//本地服务器的ip地址
[GLOBAL]
Openni ngTime =09:00                  ;//开机工作时间
Closi ngTime=22:00                   ;//停止工作时间
TimeZ one=+ 8         						                ;//所在时区，与标准时间的时间差
[WORKI NGMODE]
Working				Mode=1           			            ;//工作模式，0--单传感器模式，1--CARTVU模式
ISNu mber=3                          ;//传感器I的数量,可以是1--3
